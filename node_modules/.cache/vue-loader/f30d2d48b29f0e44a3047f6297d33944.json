"form\"\n                          lazy-validation\n                        >\n                          <v-text-field\n                            label=\"Login\"\n                            name=\"login\"\n                            v-model=\"objFormLogin.login\"\n                            prepend-inner-icon=\"mdi-account\"\n                            type=\"text\"\n                            :rules=\"loginRules\"\n                          />\n                          <v-text-field\n                            label=\"Senha\"\n                            v-model=\"objFormLogin.senha\"\n                            prepend-inner-icon=\"mdi-form-textbox-password\"\n                            type=\"password\"\n                            color=\"blue darken-3\"\n                            :rules=\"passwordRules\"\n                          />\n                          <div class=\"text-center mt3\">\n                            <v-btn @click=\"logar\" color=\"teal darken-4\" dark>\n                              <v-icon left> mdi-login-variant </v-icon>\n                              Logar\n                            </v-btn>\n                          </div>\n                        </v-form>\n                      </v-card-text>\n                    </v-col>\n                  </v-row>\n                </v-window-item>\n\n                <v-window-item :value=\"2\" transition=\"scale-transition\">\n                  <v-row>\n                    <v-col class=\"white rounded-lg\">\n                      <v-card-text class=\"text--white mt-2\">\n                        <h3 class=\"text-center Heading 4\">\n                          Cadastro de Exames.\n                        </h3>\n                        <p class=\"text-center Heading 6\">\n                          Informe os dados para gerar o pdf do exame.\n                        </p>\n\n                        <v-form\n                          v-on:submit.prevent=\"logar(objForm)\"\n                          ref=\"form\"\n                          lazy-validation\n                        >\n                          <v-row>\n                            <v-col>\n                              <v-text-field\n                                label=\"Nome do tutor\"\n                                :counter=\"30\"\n                                v-model=\"objForm.nomeTutor\"\n                                type=\"text\"\n                                :rules=\"regrasTutor\"\n                              />\n                            </v-col> </v-row\n                          ><v-row>\n                            <v-col>\n                              <v-text-field\n                                label=\"Nome do animal\"\n                                :counter=\"30\"\n                                v-model=\"objForm.nomeAnimal\"\n                                type=\"text\"\n                                :rules=\"regrasAnimal\"\n                              />\n                            </v-col>\n                          </v-row>\n                          <v-row>\n                            <v-col>\n                              <v-text-field\n                                label=\"Espécie\"\n                                v-model=\"objForm.especie\"\n                                type=\"text\"\n                                :rules=\"regrasEspecie\"\n                              />\n                            </v-col>\n                            <v-col>\n                              <v-text-field\n                                label=\"Raça\"\n                                v-model=\"objForm.raca\"\n                                type=\"text\"\n                                :rules=\"regrasRaca\"\n                              />\n                            </v-col>\n                          </v-row>\n\n                          <v-row>\n                            <v-col>\n                              <v-text-field\n                                label=\"Idade\"\n                                v-model=\"objForm.idade\"\n                                type=\"text\"\n                                :rules=\"regrasIdade\"\n                              />\n                            </v-col>\n                            <v-col>\n                              <v-text-field\n                                label=\"Gênero\"\n                                v-model=\"objForm.genero\"\n                                type=\"text\"\n                                :rules=\"regrasGenero\"\n                              />\n                            </v-col>\n                          </v-row>\n\n                          <v-row>\n                            <v-col>\n                              <v-textarea\n                                label=\"Laudo/Observações\"\n                                v-model=\"objForm.laudo\"\n                                :counter=\"860\"\n                                :rules=\"regrasLaudo\"\n                              ></v-textarea>\n                            </v-col>\n                          </v-row>\n\n                          <v-tabs\n                            v-model=\"tab\"\n                            background-color=\"teal darken-4\"\n                            dark\n                            fixed-tabs\n                          >\n                            <v-tabs-slider></v-tabs-slider>\n\n                            <v-tab v-for=\"i in itensTituloTabs\" :key=\"i.id\">\n                              <!--<div>{{ i.nome }}</div>-->\n                              <v-icon large>{{ i.icon }}</v-icon>\n                            </v-tab>\n                          </v-tabs>\n                          <v-tabs-items class=\"elevation-5\" v-model=\"tab\">\n                            <v-tab-item>\n                              <v-row v-show=\"showCamera\">\n                                <v-col>\n                                  <center>\n                                    <div\n                                      style=\"max-width: 60%; max-height: 60%\"\n                                    >\n                                      <p class=\"Heading 4\">\n                                        Capturar resultado do exame\n                                      </p>\n\n                                      <vue-web-cam\n                                        ref=\"webcam\"\n                                        :device-id=\"deviceId\"\n                                        width=\"100%\"\n                                        @started=\"onStarted\"\n                                        @stopped=\"onStopped\"\n                                        @error=\"onError\"\n                                        @cameras=\"onCameras\"\n                                        @camera-change=\"onCameraChange\"\n                                      />\n                                      <p class=\"Heading 4\">Câmera utilizada</p>\n                                      <p>\n                                        <code v-if=\"device\">{{\n                                          device.label\n                                        }}</code>\n                                        <v-select\n                                          label=\"Lista camera\"\n                                          item-text=\"label\"\n                                          item-value=\"deviceId\"\n                                          :items=\"device\"\n                                        ></v-select>\n\n                                        <v-btn\n                                          color=\"teal darken-4\"\n                                          dark\n                                          @click=\"onCapture\"\n                                          >Capture Image\n                                          <v-icon>mdi-camera-iris</v-icon>\n                                        </v-btn>\n                                        <v-btn\n                                          color=\"teal darken-4\"\n                                          dark\n                                          @click=\"onStop\"\n                                        >\n                                          <v-icon>mdi-camera-off</v-icon>\n                                          Stop Camera\n                                        </v-btn>\n                                        <v-btn\n                                          color=\"teal darken-4\"\n                                          dark\n                                          @click=\"onStart\"\n                                        >\n                                          <v-icon>mdi-camera-retake</v-icon>\n                                          Start Camera\n                                        </v-btn>\n                                      </p>\n                                    </div>\n                                  </center>\n                                </v-col>\n                              </v-row>\n\n                              <v-row v-show=\"showCortarImagem\">\n                                <v-col>\n                                  <center>\n                                    <div\n                                      class=\"text-center mt3\"\n                                      style=\"max-width: 60%; max-height: 60%\"\n                                    >\n                                      <p class=\"Heading 4\">Ajustar imagem</p>\n                                      <p>\n                                        <clipper-basic\n                                          ref=\"clipper\"\n                                          class=\"my-clipper\"\n                                          preview=\"fixed-preview\"\n                                          :src=\"imageBase64\"\n                                          :rotate=\"rotation\"\n                                        />\n                                        <clipper-range\n                                          v-model=\"rotation\"\n                                          :min=\"0\"\n                                          :max=\"360\"\n                                        ></clipper-range>\n                                      </p>\n                                      <p>\n                                        <v-btn\n                                          @click.native=\"cortar\"\n                                          color=\"teal darken-4\"\n                                          dark\n                                        >\n                                          Cortar imagem\n                                          <v-icon>mdi-content-cut</v-icon>\n                                        </v-btn>\n                                        <v-btn\n                                          @click.native=\"cancelarImagem\"\n                                          color=\"teal darken-4\"\n                                          dark\n                                        >\n                                          cancelar\n                                          <v-icon>mdi-cancel</v-icon>\n                                        </v-btn>\n                                      </p>\n                                    </div>\n                                  </center>\n                                </v-col>\n                              </v-row>\n\n                              <v-row v-show=\"showImagemFinal\">\n                                <v-col>\n                                  <center>\n                                    <div\n                                      class=\"text-center mt3\"\n                                      style=\"max-width: 60%; max-height: 60%\"\n                                    >\n                                      <p class=\"Heading 4\">\n                                        Imagem pronta para salvar\n                                      </p>\n                                      <p>\n                                        <img\n                                          :src=\"result\"\n                                          alt=\"no result\"\n                                          style=\"\n                                            max-width: 60%;\n                                            max-height: 60%;\n                                          \"\n                                        />\n                                      </p>\n                                      <p>\n                                        <v-btn\n                                          @click.native=\"cancelarImagem\"\n                                          color=\"teal darken-4\"\n                                          dark\n                                        >\n                                          cancelar\n                                          <v-icon>mdi-cancel</v-icon>\n                                        </v-btn>\n                                      </p>\n                                    </div>\n                                  </center>\n                                </v-col>\n                              </v-row>\n                            </v-tab-item>\n                          </v-tabs-items>\n\n                          <v-row>\n                            <v-col>\n                              <div class=\"mt3 pa-5\">\n                                <center>\n                                  <v-btn\n                                    @click=\"logar\"\n                                    color=\"teal darken-4\"\n                                    dark\n                                  >\n                                    <v-icon left> mdi-content-save-all </v-icon>\n                                    Salvar\n                                  </v-btn>\n                                </center>\n                              </div>\n                            </v-col>\n                          </v-row>\n                        </v-form>\n                      </v-card-text>\n                    </v-col>\n                  </v-row>\n                </v-window-item>\n              </v-window>\n            </v-card>\n          </v-col>\n        </v-row>\n      </v-container>\n    </v-main>\n    <v-overlay :value=\"overlay\">\n      <v-progress-circular indeterminate size=\"64\"></v-progress-circular>\n    </v-overlay>\n  </v-app>\n</template>\n<script>\nimport { WebCam } from \"vue-web-cam\";\nimport { clipperBasic, clipperRange } from \"vuejs-clipper\";\n\n//import { clipperFixed, clipperPreview, clipperRange   } from 'vuejs-clipper'\n\nexport default {\n  /* http://materialdesignicons.com/cdn/2.0.46/ */\n  /* https://github.com/kelvin2go/vue-cam-vision */\n\n  //cropd https://codepen.io/timtnlee/pen/gVaOrR\n\n  components: {\n    \"vue-web-cam\": WebCam,\n    clipperBasic,\n    //clipperFixed,\n    //clipperPreview,\n    clipperRange,\n  },\n  data: () => ({\n    tab: null,\n    selectedCursor: \"\",\n\n    showCamera: true,\n    showCortarImagem: false,\n    showImagemFinal: false,\n\n    //crope\n    rotation: 0,\n    src: \"\",\n    result: \"\",\n    pixel: \"\",\n    //crope\n\n    itensTituloTabs: [{ id: 0, nome: \"Câmera\", icon: \"mdi-camera\" }],\n    listaVeterinaria: [\"veterinaria1\", \"veterinaria2\", \"veterinaria3\"],\n\n    imageBase64:\n      \"data:image/svg+xml;base64,\",\n    step: 1,\n    overlay: false,\n\n    objFormLogin: {\n      login: \"\",\n      senha: \"\",\n    },\n\n    objForm: {\n      nomeTutor: \"\",\n      nomeAnimal: \"\",\n      especie: \"\",\n      raca: \"\",\n      idade: \"\",\n      genero: \"\",\n      laudo: \"\",\n    },\n\n    objFormRecuperaSenha: {\n      email: \"\",\n    },\n\n    loginRules: [(v) => !!v || \"Login é obrigatorio\"],\n    nomeClienteRules: [(v) => !!v || \"Nome do cliente é obrigatorio\"],\n    passwordRules: [(v) => !!v || \"Senha é obrigatorio\"],\n\n    regrasTutor: [\n      (v) => !!v || \"nome do tutor e obrigatório\",\n      (v) => (v && v.length <= 30) || \"O nome deve ter até de 30 caracteres\",\n    ],\n    regrasAnimal: [\n      (v) => !!v || \"nome do animal e obrigatório\",\n      (v) => (v && v.length <= 30) || \"O nome deve ter até de 30 caracteres\",\n    ],\n    regrasEspecie: [(v) => !!v || \"espécie do animal é obrigatório\"],\n    regrasRaca: [(v) => !!v || \"raça do animal é obrigatório\"],\n    regrasIdade: [(v) => !!v || \"idade do animal é obrigatório\"],\n    regrasGenero: [(v) => !!v || \"gênero do animal é obrigatório\"],\n    regrasLaudo: [\n      (v) => !!v || \"laudo-observação do exame é obrigatório\",\n      (v) =>\n        (v && v.length <= 860) ||\n        \"O laudo-observação deve ter até de 860 caracteres\",\n    ],\n\n    /*CAM*/\n    camera: null,\n    deviceId: null,\n    devices: [],\n\n    /*CAM*/\n  }),\n  props: {\n    source: String,\n  },\n\n  beforeCreate: function () {\n    // the data object is not yet created\n    this.overlay = true;\n    //this.$i18n.locale =\n  },\n\n  Create: function () {\n    // the data object is not yet created\n    this.overlay = false;\n  },\n\n  computed: {\n    device: function () {\n      return this.devices.find((n) => n.deviceId === this.deviceId);\n    },\n  },\n\n  watch: {\n    camera: function (id) {\n      this.deviceId = id;\n    },\n    devices: function () {\n      // Once we have a list select the first one\n      const [first] = this.devices;\n      if (first) {\n        this.camera = first.deviceId;\n        this.deviceId = first.deviceId;\n      }\n    },\n  },\n\n  methods: {\n    /*async onCapture() {\n      this.selectedCursor = \"wait\";\n      //console.log(\"fim do carregamento da imagem\");\n      this.imageBase64 = await this.$refs.webcam.capture();\n      this.con();\n    },*/\n\n    onCapture() {\n      this.imageBase64 = this.$refs.webcam.capture();\n      this.con();\n    },\n    onStarted(stream) {\n      console.log(\"On Started Event\", stream);\n    },\n    onStopped(stream) {\n      console.log(\"On Stopped Event\", stream);\n    },\n    onStop() {\n      this.$refs.webcam.stop();\n    },\n    onStart() {\n      this.$refs.webcam.start();\n    },\n    onError(error) {\n      console.log(\"On Error Event\", error);\n    },\n    onCameras(cameras) {\n      this.devices = cameras;\n      console.log(\"On Cameras Event\", cameras);\n    },\n    onCameraChange(deviceId) {\n      this.deviceId = deviceId;\n      this.camera = deviceId;\n      console.log(\"On Camera Change Event\", deviceId);\n    },\n\n    createBase64: function (img) {\n      var reader = new FileReader();\n      reader.readAsDataURL(img);\n\n      reader.onload = (event) => {\n        this.imageBase64 = event.target.result;\n      };\n\n      reader.onerror = (error) => {\n        console.log(\"Error: \", error);\n      };\n    },\n\n    con: function () {\n      setTimeout(() => {\n        (this.showCamera = false), this.definir();\n      }, 200);\n    },\n    definir: function () {\n      this.$refs.clipper.setTL$.next({ left: 10, top: 0 }); // percentage 0%\n      this.$refs.clipper.setTL$.next({ right: 25, bottom: 0 });\n      this.$refs.clipper.setWH$.next({ width: 50, height: 100 });\n      //this.$refs.clipper.onChange$.subscribe(() => {\n      // This happens whenever zooming, moving, and rotating occur.\n      //});\n      this.selectedCursor = \"\";\n      this.showCortarImagem = true;\n    },\n    cortar: function () {\n      const canvas = this.$refs.clipper.clip({ maxWPixel: 500 });\n      this.result = canvas.toDataURL(\"image/png\");\n      this.showCortarImagem = false;\n      this.showCamera = false;\n      this.showImagemFinal = true;\n\n      //console.log(this.$refs);\n      /*const canvas = this.$refs.clipper.clip({maxWPixel: 500})\n            this.pixel = `${canvas.width} x ${canvas.height}`;\n            this.result = canvas.toDataURL('image/jpeg');*/\n    },\n    cancelarImagem: function () {\n      this.result = null;\n      this.showCortarImagem = false;\n      this.showCamera = true;\n      this.showImagemFinal = false;\n    },\n\n    /*logar: function () {\n      this.overlay = true;\n      setTimeout(() => this.$router.push(\"modulos\"), 3000);\n    },*/\n\n    logar: function () {\n      this.overlay = true;\n      //CryptoProvider.AES.encrypt('my message', 'secret key 123').toString();\n\n      if (this.validate()) {\n        if (\n          this.objFormLogin.login == \"admin\" &&\n          this.objFormLogin.senha == \"admin\"\n        ) {\n          this.step++;\n          this.overlay = false;\n        } else {\n          this.overlay = false;\n          this.$dialog.message.error(\"Usuario ou senha inválido\", {\n            position: \"top-right\",\n            timeout: 5000,\n          });\n        }\n\n        /*\n        var objPost = {\n          email: this.objForm.email,\n          password: this.objForm.senha,\n        };\n\n        this.$axios.post(this.urlAPI, objPost).then(\n          (response) => {\n            if (response.status == 200) {\n              sessionStorage.setItem(\"logado\", 1);\n              sessionStorage.setItem(\"token\", response.data.token);\n              sessionStorage.setItem(\n                \"permissoesExecucao\",\n                JSON.stringify(response.data.dashboard.modules)\n              );\n\n              //olha o npm install idle-vue pra expirar sessao vue\n              //console.log(response.data.expires_in); ve isso aqui pra encerrar seção\n              let dt = new Date(parseInt(response.data.expires_in) * 1000);\n              console.log(dt);\n              console.log(\n                dt.getHours.toString() + \":\" + dt.getMinutes.toString()\n              );\n\n              //console.log(response.data.dashboard.modules[0]);\n\n              //console.log(response.data.token);\n              //console.log(response.data.dashboard.modules);\n              this.$router.push(\"modulos\");\n            } else {\n              this.overlay = false;\n              //console.log(response);\n              this.$dialog.message.error(response.toString(), {\n                position: \"top-right\",\n                timeout: 5000,\n              });\n            }\n          },\n          (error) => {\n            try {\n              if (error.response.data.code == 401) {\n                this.overlay = false;\n\n\n                this.$dialog.notify.error(\"E-mail ou senha inválidos!\", {\n                  position: \"top\",\n                  timeout: 5000,\n                });\n\n              } else {\n                this.overlay = false;\n                this.$dialog.message.error(error.response.data.message, {\n                  position: \"top\",\n                  timeout: 5000,\n                });\n              }\n            } catch (error) {\n              this.overlay = false;\n              this.$dialog.message.error(\"Erro de conexao! \" + this.urlAPI, {\n                position: \"top-right\",\n                timeout: 5000,\n              });\n            }\n\n            //console.log(error.response.data.code);\n\n            //this.$router.push(\"modulos\");\n          }\n        );*/\n      } else {\n        this.overlay = false;\n      }\n    },\n\n    validate: function () {\n      return this.$refs.form.validate();\n    },\n\n    reset: function () {\n      this.$refs.form.reset();\n      this.objForm.login = \"\";\n      this.objForm.senha = \"\";\n    },\n\n    resetValidation: function () {\n      this.$refs.form.resetValidation();\n    },\n  },\n};\n//this.$router.push('blog');\n</script>\n<style>\n.my-clipper {\n  width: 100%;\n  max-width: 700px;\n}\n</style>\n"]}]}
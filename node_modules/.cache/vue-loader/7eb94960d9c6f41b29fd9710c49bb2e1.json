"\n      >\n        <v-row align=\"center\" justify=\"center\">\n          <v-col cols=\"12\" xs=\"8\" sm=\"8\" md=\"8\" lg=\"8\" xl=\"8\">\n            <v-card class=\"elevation-12\">\n              <v-window v-model=\"step\">\n                <v-window-item :value=\"1\" transition=\"fade-transition\">\n                  <v-row>\n                    <v-col\n                      cols=\"12\"\n                      xs=\"12\"\n                      sm=\"4\"\n                      md=\"4\"\n                      lg=\"4\"\n                      xl=\"4\"\n                      class=\"teal darken-4 rounded-l-lg\"\n                    >\n                      <v-card-text class=\"white--text mt-2\">\n                        <h1 class=\"text-center display-1\">Petquenos</h1>\n                        <div class=\"text-center\">\n                          <v-avatar\n                            class=\"my-3 elevation-12\"\n                            color=\"white\"\n                            size=\"150\"\n                          >\n                            <img src=\"@/assets/logo.svg\" />\n                          </v-avatar>\n                        </div>\n                      </v-card-text>\n                    </v-col>\n\n                    <v-col\n                      cols=\"12\"\n                      xs=\"12\"\n                      sm=\"8\"\n                      md=\"8\"\n                      lg=\"8\"\n                      xl=\"8\"\n                      class=\"white rounded-r-lg\"\n                    >\n                      <v-card-text class=\"text--white mt-2\">\n                        <h3 class=\"text-center Heading 4\">\n                          Acesso ao sistema.\n                        </h3>\n                        <p class=\"text-center Heading 6\">\n                          Informe as credenciais para acessar o sistema!\n                        </p>\n                        <v-form\n                          v-on:submit.prevent=\"logar(objFormLogin)\"\n                          ref=\"formLogin\"\n                          lazy-validation\n                        >\n                          <v-text-field\n                            label=\"Login\"\n                            name=\"login\"\n                            v-model=\"objFormLogin.login\"\n                            prepend-inner-icon=\"mdi-account\"\n                            type=\"text\"\n                            :rules=\"loginRules\"\n                          />\n                          <v-text-field\n                            label=\"Senha\"\n                            v-model=\"objFormLogin.senha\"\n                            prepend-inner-icon=\"mdi-form-textbox-password\"\n                            type=\"password\"\n                            color=\"blue darken-3\"\n                            :rules=\"passwordRules\"\n                          />\n                          <div class=\"text-center mt3\">\n                            <v-btn @click=\"logar\" color=\"teal darken-4\" dark>\n                              <v-icon left> mdi-login-variant </v-icon>\n                              Logar\n                            </v-btn>\n                          </div>\n                        </v-form>\n                      </v-card-text>\n                    </v-col>\n                  </v-row>\n                </v-window-item>\n\n                <v-window-item :value=\"2\" transition=\"scale-transition\">\n                  <v-row>\n                    <v-col class=\"white rounded-lg\">\n                      <v-card-text class=\"text--white mt-2\">\n                        <h3 class=\"text-center Heading 4\">\n                          Cadastro de Exames.\n                        </h3>\n                        <p class=\"text-center Heading 6\">\n                          Informe os dados para gerar o pdf do exame.\n                        </p>\n\n                        <v-form\n                          v-on:submit.prevent=\"logar(objForm)\"\n                          ref=\"formPDF\"\n                          lazy-validation\n                        >\n                          <v-row>\n                            <v-col>\n                              <v-text-field\n                                label=\"Nome do tutor\"\n                                :counter=\"30\"\n                                outlined\n                                dense\n                                v-model=\"objForm.nomeTutor\"\n                                type=\"text\"\n                                :rules=\"regrasTutor\"\n                              />\n                            </v-col> </v-row\n                          ><v-row>\n                            <v-col>\n                              <v-text-field\n                                label=\"Nome do animal\"\n                                :counter=\"30\"\n                                outlined\n                                dense\n                                v-model=\"objForm.nomeAnimal\"\n                                type=\"text\"\n                                :rules=\"regrasAnimal\"\n                              />\n                            </v-col>\n                          </v-row>\n                          <v-row>\n                            <v-col>\n                              <v-select\n                                :items=\"itensEspecies\"\n                                label=\"Espécie\"\n                                v-model=\"objForm.especie\"\n                                dense\n                                outlined\n                                :rules=\"regrasEspecie\"\n                              ></v-select>\n                              <!--<v-text-field\n                                label=\"Espécie LISTA\"\n                                v-model=\"objForm.especie\"\n                                type=\"text\"\n                                :rules=\"regrasEspecie\"\n                              />-->\n                            </v-col>\n                            <v-col>\n                              <v-text-field\n                                label=\"Raça\"\n                                v-model=\"objForm.raca\"\n                                type=\"text\"\n                                dense\n                                outlined\n                                :rules=\"regrasRaca\"\n                              />\n                            </v-col>\n                          </v-row>\n\n                          <v-row>\n                            <v-col>\n                              <v-text-field\n                                label=\"Idade\"\n                                v-model=\"objForm.idade\"\n                                type=\"text\"\n                                dense\n                                outlined\n                                :rules=\"regrasIdade\"\n                              />\n                            </v-col>\n                            <v-col>\n                              <v-select\n                                :items=\"itensGenero\"\n                                label=\"Gênero\"\n                                v-model=\"objForm.genero\"\n                                dense\n                                outlined\n                                :rules=\"regrasGenero\"\n                              ></v-select>\n                            </v-col>\n                          </v-row>\n\n                          <v-row>\n                            <v-col>\n                              <v-textarea\n                                label=\"Laudo/Observações\"\n                                v-model=\"objForm.laudo\"\n                                :counter=\"860\"\n                                dense\n                                outlined\n                                :rules=\"regrasLaudo\"\n                              ></v-textarea>\n                            </v-col>\n                          </v-row>\n\n                          <v-tabs\n                            v-model=\"tab\"\n                            background-color=\"teal darken-4\"\n                            dark\n                            fixed-tabs\n                          >\n                            <v-tabs-slider></v-tabs-slider>\n\n                            <v-tab v-for=\"i in itensTituloTabs\" :key=\"i.id\">\n                              <!--<div>{{ i.nome }}</div>-->\n                              <v-icon large>{{ i.icon }}</v-icon>\n                            </v-tab>\n                          </v-tabs>\n                          <v-tabs-items class=\"elevation-5\" v-model=\"tab\">\n                            <v-tab-item>\n                              <v-row v-show=\"showCamera\">\n                                <v-col>\n                                  <center>\n                                    <div\n                                      style=\"max-width: 60%; max-height: 60%\"\n                                    >\n                                      <v-row>\n                                        <p class=\"Heading 4\">\n                                          Capturar resultado do exame\n                                        </p>\n\n                                        <vue-web-cam\n                                          ref=\"webcam\"\n                                          :device-id=\"deviceId\"\n                                          width=\"100%\"\n                                          @started=\"onStarted\"\n                                          @stopped=\"onStopped\"\n                                          @error=\"onError\"\n                                          @cameras=\"onCameras\"\n                                          @camera-change=\"onCameraChange\"\n                                        />\n                                      </v-row>\n                                      <v-row>\n                                        <!-- <code v-if=\"device\"\n                                          >Câmera utilizada\n                                          {{ device.label }}</code\n                                        > -->\n                                        <!-- <v-select\n                                          label=\"Lista camera\"\n                                          item-text=\"label\"\n                                          item-value=\"deviceId\"\n                                          :items=\"devices\"\n                                          v-model=\"camera\"\n                                        ></v-select> -->\n                                      </v-row>\n                                      <v-row>\n                                        <v-col>\n                                          <v-btn\n                                            color=\"teal darken-4\"\n                                            dark\n                                            @click=\"onCapture\"\n                                            >Capture\n                                            <v-icon>mdi-camera-iris</v-icon>\n                                          </v-btn>\n                                        </v-col>\n                                        <!--<v-col>\n                                          <v-btn\n                                            color=\"teal darken-4\"\n                                            dark\n                                            @click=\"onStop\"\n                                          >\n                                            <v-icon>mdi-camera-off</v-icon>\n                                            Stop\n                                          </v-btn>\n                                        </v-col>\n                                        <v-col>\n                                          <v-btn\n                                            color=\"teal darken-4\"\n                                            dark\n                                            @click=\"onStart\"\n                                          >\n                                            <v-icon>mdi-camera-retake</v-icon>\n                                            Start\n                                          </v-btn>\n                                        </v-col>-->\n                                      </v-row>\n                                    </div>\n                                  </center>\n                                </v-col>\n                              </v-row>\n\n                              <v-row v-show=\"showCortarImagem\">\n                                <v-col>\n                                  <center>\n                                    <div\n                                      class=\"text-center mt3\"\n                                      style=\"max-width: 60%; max-height: 60%\"\n                                    >\n                                      <p class=\"Heading 4\">Ajustar imagem</p>\n                                      <!--<clipper-fixed\n                                        ref=\"clipper\"\n                                        :wrap-ratio=\"1\"\n                                        :src=\"imageBase64\"\n                                        @load=\"imgLoad\"\n                                      />-->\n                                      <!--<clipper-basic\n                                        ref=\"clipper\"\n                                        class=\"my-clipper\"\n                                        preview=\"fixed-preview\"\n                                        :src=\"imageBase64\"\n                                        :rotate=\"rotation\"\n                                      />\n                                      <clipper-range\n                                        v-model=\"rotation\"\n                                        :min=\"0\"\n                                        :max=\"360\"\n                                      ></clipper-range>-->\n                                      <cropper\n                                        class=\"cropper\"\n                                        :src=\"imageBase64\"\n                                        :touchResize=\"false\"\n                                        :mouseMove=\"false\"\n                                        @change=\"changeCrooper\"\n                                      />\n\n                                      <v-row>\n                                        <v-col>\n                                          <v-btn\n                                            @click.native=\"cortar\"\n                                            color=\"teal darken-4\"\n                                            dark\n                                          >\n                                            Cortar\n                                            <v-icon>mdi-content-cut</v-icon>\n                                          </v-btn>\n                                        </v-col>\n                                        <v-col>\n                                          <v-btn\n                                            @click.native=\"cancelarImagem\"\n                                            color=\"teal darken-4\"\n                                            dark\n                                          >\n                                            cancelar\n                                            <v-icon>mdi-cancel</v-icon>\n                                          </v-btn>\n                                        </v-col>\n                                      </v-row>\n                                    </div>\n                                  </center>\n                                </v-col>\n                              </v-row>\n\n                              <v-row v-show=\"showImagemFinal\">\n                                <v-col>\n                                  <center>\n                                    <div\n                                      class=\"text-center mt3\"\n                                      style=\"max-width: 60%; max-height: 60%\"\n                                    >\n                                      <p class=\"Heading 4\">\n                                        Imagem pronta para salvar\n                                      </p>\n                                      <p>\n                                        <img\n                                          :src=\"result\"\n                                          alt=\"no result\"\n                                          style=\"\n                                            max-width: 60%;\n                                            max-height: 60%;\n                                          \"\n                                        />\n                                      </p>\n                                      <p>\n                                        <v-btn\n                                          @click.native=\"cancelarImagem\"\n                                          color=\"teal darken-4\"\n                                          dark\n                                        >\n                                          cancelar\n                                          <v-icon>mdi-cancel</v-icon>\n                                        </v-btn>\n                                      </p>\n                                    </div>\n                                  </center>\n                                </v-col>\n                              </v-row>\n                            </v-tab-item>\n                          </v-tabs-items>\n\n                          <v-row>\n                            <v-col>\n                              <div class=\"mt3 pa-5\">\n                                <center>\n                                  <v-btn\n                                    @click=\"gerarPDF\"\n                                    color=\"teal darken-4\"\n                                    dark\n                                  >\n                                    <v-icon left>mdi-file-pdf </v-icon>\n                                    Baixar PDF\n                                  </v-btn>\n                                </center>\n                              </div>\n                            </v-col>\n                          </v-row>\n                        </v-form>\n                      </v-card-text>\n                    </v-col>\n                  </v-row>\n                </v-window-item>\n              </v-window>\n            </v-card>\n          </v-col>\n        </v-row>\n      </v-container>\n    </v-main>\n    <v-overlay :value=\"overlay\">\n      <v-progress-circular indeterminate size=\"64\"></v-progress-circular>\n    </v-overlay>\n  </v-app>\n</template>\n<script>\nimport { WebCam } from \"vue-web-cam\";\n//import { clipperFixed, clipperBasic, clipperRange } from \"vuejs-clipper\";\n//import { clipperBasic, clipperRange } from \"vuejs-clipper\";\nimport { Cropper } from \"vue-advanced-cropper\";\n\nexport default {\n  components: {\n    \"vue-web-cam\": WebCam,\n    //clipperFixed,clipperBasic,clipperRange,\n    Cropper,\n  },\n  data: () => ({\n    tab: null,\n    inicializacao: true,\n\n    /*CAM*/\n    camera: null,\n    deviceId: null,\n    devices: [],\n    /*CAM*/\n\n    selectedCursor: \"\",\n\n    showCamera: true,\n    showCortarImagem: false,\n    showImagemFinal: false,\n\n    //crope\n    rotation: 0,\n    imageBase64: \"\",\n    src: \"\",\n    result: \"\",\n    //crope\n    itensEspecies: [\"Canina\", \"Felina\"],\n    itensGenero: [\"Macho\", \"Fêmea\"],\n\n    itensTituloTabs: [{ id: 0, nome: \"Câmera\", icon: \"mdi-camera\" }],\n\n    step: 1,\n    overlay: false,\n\n    objFormLogin: {\n      login: \"\",\n      senha: \"\",\n    },\n\n    objForm: {\n      nomeTutor: \"\",\n      nomeAnimal: \"\",\n      especie: \"\",\n      raca: \"\",\n      idade: \"\",\n      genero: \"\",\n      laudo: \"\",\n    },\n\n    loginRules: [(v) => !!v || \"Login é obrigatorio\"],\n    passwordRules: [(v) => !!v || \"Senha é obrigatorio\"],\n\n    regrasTutor: [\n      (v) => !!v || \"nome do tutor e obrigatório\",\n      (v) => (v && v.length <= 30) || \"O nome deve ter até de 30 caracteres\",\n    ],\n    regrasAnimal: [\n      (v) => !!v || \"nome do animal e obrigatório\",\n      (v) => (v && v.length <= 30) || \"O nome deve ter até de 30 caracteres\",\n    ],\n    regrasEspecie: [(v) => !!v || \"espécie do animal é obrigatório\"],\n    regrasRaca: [(v) => !!v || \"raça do animal é obrigatório\"],\n    regrasIdade: [(v) => !!v || \"idade do animal é obrigatório\"],\n    regrasGenero: [(v) => !!v || \"gênero do animal é obrigatório\"],\n    regrasLaudo: [\n      (v) => !!v || \"laudo-observação do exame é obrigatório\",\n      (v) =>\n        (v && v.length <= 860) ||\n        \"O laudo-observação deve ter até de 860 caracteres\",\n    ],\n  }),\n\n  computed: {\n    device: function () {\n      let list = this.devices.find((n) => n.deviceId === this.deviceId);\n\n      return list;\n    },\n  },\n\n  watch: {\n    camera: function (id) {\n      this.deviceId = id;\n    },\n    devices: function () {\n      // Once we have a list select the first one\n      const [first] = this.devices;\n\n      if (first) {\n        this.camera = first.deviceId;\n        this.deviceId = first.deviceId;\n      }\n    },\n  },\n\n  methods: {\n    changeCrooper({ coordinates, canvas }) {\n      console.log(coordinates, canvas);\n    },\n\n    imgLoad: function () {\n      const imgRatio = this.$refs.clipper.imgRatio;\n      if (imgRatio < 1) this.based = this.maxHeight * imgRatio;\n      else this.based = this.maxWidth;\n    },\n    onCapture() {\n      this.imageBase64 = this.$refs.webcam.capture();\n      this.con();\n    },\n    onStarted(stream) {\n      console.log(\"On Started Event\", stream);\n    },\n    onStopped(stream) {\n      console.log(\"On Stopped Event\", stream);\n    },\n    onStop() {\n      this.$refs.webcam.stop();\n    },\n    onStart() {\n      this.$refs.webcam.start();\n    },\n    onError(error) {\n      console.log(\"On Error Event\", error);\n    },\n    onCameras(cameras) {\n      this.devices = cameras;\n      //console.log(\"On Cameras Event\", cameras);\n    },\n    onCameraChange(deviceId) {\n      if (this.inicializacao) {\n        let cam = this.devices[this.devices.length - 1];\n        this.deviceId = cam.deviceId;\n        this.camera = cam.deviceId;\n        this.inicializacao = false;\n      } else {\n        this.deviceId = deviceId;\n        this.camera = deviceId;\n      }\n\n      console.log(\"On Camera Change Event\", deviceId);\n    },\n\n    con: function () {\n      setTimeout(() => {\n        (this.showCamera = false), this.definir();\n      }, 200);\n    },\n    definir: function () {\n      //this.$refs.clipper.setTL$.next({ left: 10, top: 0 }); // percentage 0%\n      //this.$refs.clipper.setTL$.next({ right: 25, bottom: 0 });\n      //this.$refs.clipper.setWH$.next({ width: 50, height: 100 });\n\n      this.selectedCursor = \"\";\n      this.showCortarImagem = true;\n    },\n    cortar: function () {\n      const canvas = this.$refs.clipper.clip({ maxWPixel: 500 });\n      this.result = canvas.toDataURL(\"image/png\");\n\n      this.showCortarImagem = false;\n      this.showCamera = false;\n      this.showImagemFinal = true;\n    },\n    cancelarImagem: function () {\n      this.result = null;\n      this.showCortarImagem = false;\n      this.showCamera = true;\n      this.showImagemFinal = false;\n    },\n\n    logar: function () {\n      this.overlay = true;\n      if (this.validateLogin()) {\n        if (\n          this.objFormLogin.login == \"admin\" &&\n          this.objFormLogin.senha == \"admin\"\n        ) {\n          this.step++;\n          this.overlay = false;\n        } else {\n          this.overlay = false;\n          this.$dialog.message.error(\"Usuario ou senha inválido\", {\n            position: \"top-right\",\n            timeout: 5000,\n          });\n        }\n      } else {\n        this.overlay = false;\n      }\n    },\n\n    gerarPDF: function () {\n      var objPost = {\n        nomeTutor: this.objForm.nomeTutor,\n        nomeAnimal: this.objForm.nomeAnimal,\n        especie: this.objForm.especie,\n        raca: this.objForm.raca,\n        idade: this.objForm.idade,\n        genero: this.objForm.genero,\n        laudo: this.objForm.laudo,\n        image: this.result,\n        login: this.login,\n      };\n\n      console.log(objPost);\n\n      this.$axios\n        .post(\"https://petquenos.com.br/apiPdf/public/generatepdf\", objPost, {\n          responseType: \"blob\",\n        })\n        .then(\n          (response) => {\n            console.log(response);\n            if (response.status == 200) {\n              const url = window.URL.createObjectURL(new Blob([response.data]));\n              const link = document.createElement(\"a\");\n              link.href = url;\n              link.setAttribute(\"download\", \"file.pdf\"); //or any other extension\n              document.body.appendChild(link);\n              link.click();\n            } else {\n              this.overlay = false;\n              //console.log(response);\n              this.$dialog.message.error(response.toString(), {\n                position: \"top-right\",\n                timeout: 5000,\n              });\n            }\n          },\n          (error) => {\n            try {\n              if (error.response.data.code == 401) {\n                //this.overlay = false;\n                /*this.$dialog.message.error(\"E-mail ou senha inválidos!\", {\n                  position: \"top\",\n                  timeout: 5000,\n                });*/\n                //this.$dialog.notify.error(\"E-mail ou senha inválidos!\", {\n                //position: \"top\",\n                //timeout: 5000,\n                //});\n                /*this.$dialog.message.info('E-mail ou senha inválidos!', {\n                  position: 'top'\n                });*/\n              } else {\n                /*this.overlay = false;\n                this.$dialog.message.error(error.response.data.message, {\n                  position: \"top\",\n                  timeout: 5000,\n                });*/\n              }\n            } catch (error) {\n              console.log(error);\n            }\n\n            //console.log(error.response.data.code);\n\n            //this.$router.push(\"modulos\");\n          }\n        );\n\n      this.overlay = true;\n      if (this.validateGerarPDF()) {\n        if (this.imageBase64.length > 0) {\n          if (this.result.length > 0) {\n            this.$dialog.message.success(\"OK\", {\n              position: \"top-right\",\n              timeout: 5000,\n            });\n          } else {\n            this.$dialog.message.error(\n              \"É necessario cortar a imagem do exame pra gerar o PDF\",\n              {\n                position: \"top-right\",\n                timeout: 5000,\n              }\n            );\n          }\n\n          this.overlay = false;\n        } else {\n          this.$dialog.message.error(\n            \"É necessario capturar a imagem do exame pra gerar o PDF\",\n            {\n              position: \"top-right\",\n              timeout: 5000,\n            }\n          );\n          this.overlay = false;\n        }\n      } else {\n        this.$dialog.message.error(\n          \"Observe o formulário, existe campos inválidos\",\n          {\n            position: \"top-right\",\n            timeout: 5000,\n          }\n        );\n        this.overlay = false;\n      }\n    },\n\n    validateLogin: function () {\n      return this.$refs.formLogin.validate();\n    },\n\n    validateGerarPDF: function () {\n      return this.$refs.formPDF.validate();\n    },\n\n    reset: function () {\n      this.$refs.form.reset();\n      this.objForm.login = \"\";\n      this.objForm.senha = \"\";\n    },\n\n    resetValidation: function () {\n      this.$refs.form.resetValidation();\n    },\n  },\n};\n</script>\n<style>\n.cropper {\n  height: 600px;\n  background: #ddd;\n}\n</style>\n"]}]}